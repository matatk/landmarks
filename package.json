{
  "name": "landmarks",
  "version": "2.7.0",
  "private": true,
  "scripts": {
    "prebuild:all": "npm test",
    "build:all": "node scripts/build.js --browser all",
    "clean:builds": "node scripts/build.js --browser all --clean-only",
    "preprofile": "rollup src/code/landmarksFinder.js -f iife --output.name=LandmarksFinder -o scripts/generated-landmarks-finder.js",
    "profile": "node scripts/profile.js",
    "profile-guarding": "node scripts/profile-guarding.js",
    "start:firefox": "web-ext run --browser-console --source-dir build/firefox/",
    "pretest": "eslint . && rollup -c test/rollup.config.js",
    "test": "ava && npm run test:css",
    "test:css": "npx stylelint --config .stylelintrc.json src/**/*.css",
    "version": "standard-version --tag-prefix="
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build:all"
    }
  },
  "ava": {
    "files": [
      "!test/generated-*.js",
      "!test/rollup.config.js"
    ]
  },
  "devDependencies": {
    "addons-linter": "^1.23.0",
    "archiver-promise": "~1.0",
    "ava": "^3.7.0",
    "chalk": "^4.0.0",
    "deepmerge": "~4.2",
    "eslint": "^6.8.0",
    "fs-extra": "^9.0.0",
    "husky": "^4.2.5",
    "jsdom": "^16.2.2",
    "one-svg-to-many-sized-pngs": "github:matatk/one-svg-to-many-sized-pngs#0.1.1",
    "page-structural-semantics-scanner-tests": "git+https://github.com/matatk/page-structural-semantics-scanner-tests.git#0.4.0",
    "puppeteer": "^3.0.0",
    "replace-in-file": "^5.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-esformatter": "^2.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "standard-version": "^7.1.0",
    "stats-lite": "~2.2",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "yargs": "^15.3.1"
  }
}
